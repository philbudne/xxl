var i = 1;
while (i <= 5) {
    System.print(i);
    i += 1;
}

System.print("break");
leave_while:
while (true) {
    i -= 1;
    System.print(i);
    if (i == 0) {
	leave_while();
    }
}

System.print("continue");
i = 0;
while (i < 5) continue: {
    i += 1;
    System.print(i);
    continue();
    System.print("here");
}
